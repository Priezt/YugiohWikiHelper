{
	"name": "YuGiOh Wiki Helper",
	"version": "1.2",
	"description": "yugioh-wiki.netの機能を強化する",
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs",
		"notifications"
	],
	"content_scripts": [
		{
			"matches": [
				"http://yugioh-wiki.net/*"
			],
			"js": [
				"lib/jquery-1.6.1.min.js",
				"lib/ztoolkit.js",
				"utf8_str.js",
				"content_main.js"
			]
		}
	],
	"background_page": "background.html",
	"icons": {
		"128": "img/icon128.png",
		"16": "img/icon16.jpg",
		"48": "img/icon48.png"
	},
	"page_action": {
		"default_icon": "img/icon16.jpg",
		"default_title": "YuGiOh Wiki Helper"
	}
}